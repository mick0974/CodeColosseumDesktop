{
  "name": "code-colosseum-desktop",
  "version": "0.0.0",
  "main": "main.js",
  "build": {
    "appId": "com.talcoteam.CoCo",
    "productName": "CodeColosseum",
    "files": [
      "**/*",
      "dist/**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "dist"
    },
    "asar": true,
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/favicon.ico"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "hotreload": "npm-run-all -p electron:serve start",
    "electron:serve": "wait-on tcp:4200 && electron . --serve",
    "electron": "electron .",
    "electron-build": "ng build && electron .",
    "electron-build-linux": "electron-builder build -l",
    "electron-build-windows": "electron-builder build -w"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@tauri-apps/api": "^1.2.0",
    "electron": "^21.3.1",
    "electron-builder": "^23.6.0",
    "electron-reload": "^2.0.0-alpha.1",
    "npm-run-all": "^4.1.5",
    "primeicons": "^3.0.0",
    "primeng": "^13.3.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "wait-on": "^6.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@tauri-apps/cli": "^1.2.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2"
  }
}
